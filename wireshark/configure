$as_echo "no" >&6; }
		;;
	*)
		{ $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }

$as_echo "#define NEED_8_BYTE_ALIGNMENT 1" >>confdefs.h

		;;
esac

